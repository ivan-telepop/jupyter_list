    "class A:\n",
    "    @classmethod\n",
    "    def foo(cls):\n",
    "        return cls\n",
    "    def bar(self):\n",
    "        return self\n",
    "    @staticmethod\n",
    "    # Just a method which is in the namespace of class A\n",
    "    def abc():\n",
    "        return \"abc staticmethod\"\n",
    "        \n",
    "\n",
    "\n",
    "b = A()\n",
    "\n",
    "\n",
    "b.foo()\n",
    "\n",
    "dir(b)\n",
    "\n",
    "A.abc()"
