roms = {'M': 1000, 'CM': 900, 'D': 500, 'CD': 400,
                 'C': 100, 'XC': 90, 'L': 50, 'XL': 40,
                 'X': 10, 'IX': 9, 'V': 5, 'IV': 4, 'I': 1}


def rom_converter(num:int):
    res = ''
    for letter, value in roms.items():
        while num >= value:
            res += letter
            num -= value
    return res


rom_converter(1980)



