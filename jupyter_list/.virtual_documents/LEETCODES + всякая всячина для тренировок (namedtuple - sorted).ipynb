data = '00011111111100000011111111100111001111000111100000111111111110000111'


def bin_finder(data):
    counted = None
    for one in data:
        if one != '1':
            counted = data.replace(one," ").split(" ")
    return len(max(counted,key=len))



print(bin_finder(data))


